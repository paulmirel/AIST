# make an array
# 3x3, filled with 0
# iterate, make ten of these filled with the index number
# stack 'em together into a 3D array
import numpy as np
print("test")
arrays = []
for index in range (0, 10):
    arrays.append(np.full(shape=(3,3),fill_value=index))
    #print( arrays[index])

three_d_array = np.stack( arrays, axis = 2 ) 
print(three_d_array.shape)